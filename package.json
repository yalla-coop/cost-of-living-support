{
  "name": "boilerplate",
  "version": "0.0.0",
  "private": false,
  "engines": {
    "node": "14.x",
    "npm": "6.x"
  },
  "scripts": {
    "dev": "cd client && npm start",
    "start": "cd client && npm start",
    "build": "cd client && npm build",
    "clean": "rm -rf dist && mkdir dist",
    "init": "npm i && cd client && npm i && npm run build"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "moment": "^2.29.1",
    "serve-favicon": "^2.5.0",
    "uniqid": "^5.3.0",
    "uuid": "^8.3.2",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "eslint": "^7.11.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.4"
  }
}
